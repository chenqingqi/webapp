/**
 * 模块-样式-通配
 */
* {
  margin: 0;
  padding: 0;
  border-color: #e9e9e9;
  color: #333333;
  font-family: DroidSans, DroidSansFallback;
  font-size: 26px;
  word-break: break-all;
  text-decoration: none;
}
html,
body {
  width: 100%;
  height: 100%;
  background-color: #F0F2F6;
}
li {
  list-style: none;
}
a {
  color: #333333;
  p-decoration: none;
}
/**
 * 审核
 */
#bar_audit {
  width: 640px;
  min-height: 50px;
  overflow: hidden;
  padding-bottom: 30px;
}
#bar_audit #icon {
  display: block;
  width: 80px;
  height: 80px;
  float: left;
  margin: 40px 0 0 285px;
}
#bar_audit #title {
  width: 100%;
  padding-top: 15px;
  float: left;
  text-align: center;
}
#bar_audit #subtitle {
  width: 100%;
  padding-top: 10px;
  float: left;
  text-align: center;
  font-size: 24px;
  color: #777;
}
#bar_audit #evaluate {
  width: 640px;
  padding-top: 10px;
  text-align: center;
  float: left;
  color: #000;
}
/**
 * 通知-公告板
 */
#panel_notice {
  width: 640px;
  min-height: 100px;
  overflow: hidden;
  background-color: #FFF;
}
#panel_notice #title {
  width: 410px;
  height: 91px;
  background-image: url(/service/img/scarf_blue.png);
  background-size: 410px 91px;
  float: left;
  margin: 45px 116px;
  text-align: center;
  line-height: 80px;
  font-size: 32px;
  color: #FFF;
}
#panel_notice #text {
  display: block;
  width: 600px;
  float: left;
  margin: 5px 25px 40px 25px;
  word-break: break-all;
  line-height: 42px;
  font-size: 24px !important;
}
