#bar_title_search #back {
    width: 88px;
    height: 88px;
    background-image: url(/resource/back.png);
    background-repeat: no-repeat;
    background-size: 50px 50px;
    background-position: 12px 16px;
    float: left;
}
#bar_title_search #back:hover {
    -webkit-filter: brightness(90%);
    filter: brightness(90%);
}
#bar_title_search #title div {
    width: 520px;
    height: 50px;
    margin: 18px 0;
    border-radius: 10px;
    background-color: #fff;
    padding: 0 20px 0 0;
    float: left;
}
#bar_title_search #title div img {
    width: 30px;
    margin: 10px 20px 0;
    float: left;
}
#bar_title_search #title div input {
    width: 80%;
    float: left;
    height: 50px;
    line-height: 40px;
    font-size: 24px;
    color: #666;
    border: 0;
    outline: 0;
}
#bar_title_search #text {
    float: right;
    line-height: 88px;
    width: 88px;
    text-align: center;
}
#bar_title_search #text span {
    display: block;
    color: #fff;
    font-size: 26px;
}
/*
 * 搜索课程显示样式
 */
	#item_course_full {
    width: 640px;
    height: 160px;
    border-bottom: 1px solid #e9e9e9;
    background-color: #FFF;
    float: left;
}


#item_course_full #logo {
    width: 230px;
    padding: 15px;
    float: left;
    overflow: hidden;
}
#item_course_full #logo img {
    width: 230px;
    height: 130px;
}
#item_course_full #container {
    width: 350px;
    height: 130px;
    margin: 15;
    float: left;
}
#item_course_full #container #title {
    width: 360px;
    height: 70px;
    line-height: 35px;
    margin-top: 8px;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
#item_course_full #container #price {
    display: inline-block;
    width: 150px;
    height: 40px;
    line-height: 40px;
    margin-top: 14px;
    font-size: 20px;
    float: left;
}
#item_course_full #container .red {
    color: #FF7E7E;
}
.red {
    color: red; 
}
.green{
	 color: green;
}
#item_course_full #container #hour {
    display: inline-block;
    width: 100px;
    height: 40px;
    margin-top: 14px;
    color: #999;
    line-height: 40px;
    font-size: 20px;
    float: right;
    background: url(/resource/folder.png) no-repeat 0 center;
    background-size: 33px 40px;
    text-indent: 30px;
}
/*
 * 进度条
 */	
#wrapper {
	width: 640px;
	margin-top: 88px;
	position: relative;
	overflow: hidden;

	/* Prevent native touch events on Windows */
	-ms-touch-action: none;

	/* Prevent the callout on tap-hold and text selection */
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;

	/* Prevent text resize on orientation change, useful for web-apps */
	-webkit-text-size-adjust: none;
	-moz-text-size-adjust: none;
	-ms-text-size-adjust: none;
	-o-text-size-adjust: none;
	text-size-adjust: none;
}

#scroller {
	height: auto;
	position: absolute;

	/* Prevent elements to be highlighted on tap */
	-webkit-tap-highlight-color: rgba(0,0,0,0);

	/* Put the scroller into the HW Compositing layer right from the start */
	-webkit-transform: translateZ(0);
	-moz-transform: translateZ(0);
	-ms-transform: translateZ(0);
	-o-transform: translateZ(0);
	transform: translateZ(0);
}
