/**
 * 导入模块样式
 */
#bind_tip {
  width: 640px;
  height: 101%;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
}
#bind_tip #panel {
  width: 560px;
  height: 400px;
  background-color: #FFF;
  margin: 0 auto;
  padding-top: 30px;
  border-radius: 10px;
}
#bind_tip #panel #account_name {
  width: 560px;
  height: 80px;
  font-size: 30px;
  text-align: center;
  line-height: 80px;
  color: #999;
}
#bind_tip #panel #account {
  width: 500px;
  height: 80px;
  margin: 0px 0 0 30px;
  border-radius: 80px;
  border: 1px solid #E9E9E9;
}
#bind_tip #panel #account img {
  float: left;
  width: 70px;
  height: 70px;
  margin: 5px 0 0 6px;
  border-radius: 50%;
}
#bind_tip #panel #account p {
  width: 420px;
  height: 88px;
  line-height: 88px;
  font-size: 30px;
  text-indent: 15px;
  color: #000;
}
#bind_tip #panel #info {
  width: 500px;
  text-align: left;
  line-height: 42px;
}
#bind_tip #panel #enter {
  width: 560px;
  height: 80px;
  margin: 30px 0 0 0;
  line-height: 80px;
  text-align: center;
  border-top: 1px solid #e9e9e9;
  color: #5baef5;
  font-size: 30px;
}
#bind_tip #panel #enter:hover {
  background-color: #E9E9E9;
}
#simple_tip {
  background-color: #333;
  padding: 15px;
  border-radius: 6px;
  position: absolute;
  z-index: 999;
  color: #fff;
}
/**
 * 模块-样式-通配
 */
* {
  margin: 0;
  padding: 0;
  overflow: hidden;
  border-color: #e9e9e9;
  color: #333333;
  font-family: DroidSansFallback, DroidSans;
  font-size: 26px;
  word-break: break-all;
  text-decoration: none;
  cursor: none;
}
html,
body {
  width: 100%;
  height: 100%;
  padding-right: 17px;
  background-color: #F0F2F6;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;
  user-select: none;
  -webkit-user-select: none;
}
body {
  display: none;
}
li {
  list-style: none;
}
a {
  color: #333;
  p-decoration: none;
}
/**
 * 白色标题栏
 */
#white_title {
  width: 640px;
  height: 88px;
  background-color: #FFF;
  border-bottom: 1px solid #E9E9E9;
}
#white_title #back {
  float: left;
  width: 88px;
  height: 88px;
  background-image: url(/resource/back_black.png);
  background-repeat: no-repeat;
  background-size: 50px 50px;
  background-position: 15px 19px;
}
#white_title #back:hover {
  -webkit-filter: brightness(90%);
  filter: brightness(90%);
}
#white_title #title {
  float: left;
  width: 464px;
  height: 88px;
  color: #333;
  text-align: center;
  line-height: 88px;
  font-size: 32px;
}
#white_title #cross {
  float: left;
  width: 88px;
  height: 88px;
  background-image: url(/resource/cross_black.png);
  background-repeat: no-repeat;
  background-size: 50px 50px;
  background-position: 15px 19px;
}
#white_title #cross:hover {
  -webkit-filter: brightness(90%);
  filter: brightness(90%);
}
#logout {
  width: 640px;
  height: 101%;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
}
#logout #panel {
  width: 560px;
  height: 230px;
  background-color: #FFF;
  margin: 0 auto;
  padding-top: 50px;
  border-radius: 10px;
}
#logout #panel #info {
  width: 560px;
  height: 80px;
  margin-left: 0;
  line-height: 80px;
  color: #333;
  font-size: 28px;
  text-align: center;
}
#logout #panel #buttons {
  width: 560px;
  height: 80px;
  margin: 30px 0 0 0;
  border-top: 1px solid #e9e9e9;
}
#logout #panel #buttons #esc {
  float: left;
  width: 278px;
  height: 78px;
  line-height: 78px;
  text-align: center;
  color: dodgerblue;
}
#logout #panel #buttons #esc:hover {
  background-color: #E9E9E9;
}
#logout #panel #buttons #enter {
  float: left;
  width: 278px;
  height: 78px;
  border-right: 1px solid #E9E9E9;
  line-height: 78px;
  text-align: center;
  color: red;
}
#logout #panel #buttons #enter:hover {
  background-color: #E9E9E9;
}
/**
 * 主页面样式 
 */
body {
  background-color: #FFF;
}
#first_name,
#next_name {
  width: 640px;
  height: 80px;
  margin-top: 80px;
  font-size: 30px;
  text-align: center;
  line-height: 80px;
  color: #999;
}
#next_name {
  margin-top: 0px;
}
#first_account,
#next_account {
  width: 560px;
  height: 80px;
  margin-left: 40px;
  border-radius: 80px;
  border: 1px solid #999;
}
#first_account img,
#next_account img {
  float: left;
  width: 70px;
  height: 70px;
  margin: 5px 0 0 6px;
  border-radius: 50%;
}
#first_account p,
#next_account p {
  width: 480px;
  height: 88px;
  line-height: 80px;
  font-size: 30px;
  text-indent: 15px;
  color: #000;
}
#icon {
  width: 640px;
  height: 120px;
  text-align: center;
}
#icon img {
  width: 60px;
  height: 60px;
  margin-top: 35px;
}
#info {
  width: 560px;
  margin-left: 40px;
  padding: 15px 0 15px 5px;
  line-height: 62px;
  font-size: 24px;
  text-align: center;
  color: #999;
}
#button {
  width: 640px;
  height: 100px;
  background-image: url(/resource/button_radius.png);
  background-size: 640px 100px;
  position: fixed;
  bottom: 0px;
  text-align: center;
  line-height: 100px;
  color: #FFF;
  font-size: 32px;
}
#button:hover {
  -webkit-filter: brightness(90%);
  filter: brightness(90%);
}
